CREATE TABLE `tbl_package_history` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT '主键id',
  `name` varchar(255) NOT NULL COMMENT '模块id,唯一',
  `pid` int(11) NOT NULL COMMENT '所属项目id',
  `title` varchar(255) NOT NULL COMMENT '模块名称',
  `desc` text NOT NULL COMMENT '描述',
  `code` longtext NOT NULL COMMENT '实现代码',
  `deps` text NOT NULL COMMENT '依赖关系',
  `filename` varchar(255) NOT NULL COMMENT '文件名',
  `compressiontype` int(11) NOT NULL COMMENT '压缩模式',
  `lastversion` varchar(255) NOT NULL COMMENT '最新版本号',
  `lastupdate` datetime NOT NULL COMMENT '最后一次更新',
  `filesize` int(11) NOT NULL COMMENT '文件大小',
  `src` varchar(255) NOT NULL COMMENT '压缩版地址',
  `targetSrc` varchar(255) NOT NULL COMMENT '原始地址',
  `lastmodify` varchar(255) NOT NULL COMMENT '最后一次修改者',
  `history_msg` varchar(255) NOT NULL COMMENT '版本信息',
  `codepath` varchar(255) NOT NULL COMMENT '文件路径',
  `useCache` tinyint(4) NOT NULL DEFAULT '0' COMMENT '使用缓存',
  `type` int(11) NOT NULL DEFAULT '1',
  `tplSource` text NOT NULL,
  `tplPath` varchar(255) NOT NULL,
  `mintype` int(11) NOT NULL DEFAULT '0',
  `lockman` varchar(255) DEFAULT NULL COMMENT '锁定人',
  `enableflag` int(11) NOT NULL DEFAULT '1',
  `babel` int(11) NOT NULL DEFAULT '0',
  `preversion` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `name_3` (`name`)
) ENGINE=InnoDB AUTO_INCREMENT=202562 DEFAULT CHARSET=utf8;
CREATE TABLE `tbl_package` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT '主键id',
  `name` varchar(255) NOT NULL COMMENT '模块id,唯一',
  `pid` int(11) NOT NULL COMMENT '所属项目id',
  `title` varchar(255) NOT NULL COMMENT '模块名称',
  `desc` text NOT NULL COMMENT '描述',
  `code` longtext NOT NULL COMMENT '实现代码',
  `deps` text NOT NULL COMMENT '依赖关系',
  `filename` varchar(255) NOT NULL COMMENT '文件名',
  `compressiontype` int(11) NOT NULL COMMENT '压缩模式',
  `lastversion` varchar(255) NOT NULL COMMENT '最新版本号',
  `lastupdate` datetime NOT NULL COMMENT '最后一次更新',
  `filesize` int(11) NOT NULL COMMENT '文件大小',
  `src` varchar(255) NOT NULL COMMENT '压缩版地址',
  `targetSrc` varchar(255) NOT NULL COMMENT '原始地址',
  `lastmodify` varchar(255) NOT NULL COMMENT '最后一次修改者',
  `codepath` varchar(255) NOT NULL COMMENT '源码路径',
  `useCache` tinyint(4) NOT NULL DEFAULT '0' COMMENT '是否用了缓存',
  `type` int(11) NOT NULL DEFAULT '1' COMMENT '类型 1普通模块 2模版模块',
  `tplSource` mediumtext NOT NULL COMMENT '模版内容',
  `tplPath` varchar(255) NOT NULL COMMENT '模版路径',
  `mintype` int(11) NOT NULL DEFAULT '0',
  `lockman` varchar(255) DEFAULT NULL COMMENT '锁定人',
  `enableflag` int(11) NOT NULL DEFAULT '1',
  `babel` int(11) NOT NULL DEFAULT '0',
  `preversion` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `name` (`name`),
  UNIQUE KEY `name_2` (`name`),
  KEY `title_index` (`title`)
) ENGINE=InnoDB AUTO_INCREMENT=11924 DEFAULT CHARSET=utf8;

CREATE TABLE `tbl_project` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT '项目id',
  `name` varchar(255) NOT NULL COMMENT '项目名称',
  `jsipath` varchar(255) NOT NULL COMMENT 'jsi路径',
  `jspath` varchar(255) NOT NULL COMMENT 'js路径',
  `members` varchar(255) NOT NULL,
  `lastupdate` datetime NOT NULL COMMENT '最后一次更新',
  `domain` varchar(255) NOT NULL COMMENT '域名',
  `iscommon` int(11) NOT NULL COMMENT '是否公共项目',
  `lastmodify` varchar(255) NOT NULL COMMENT '最后一次修改者',
  `cpath` varchar(255) NOT NULL COMMENT '控制器路径',
  `mpath` varchar(255) NOT NULL COMMENT '模型路径',
  `enableflag` tinyint(4) NOT NULL DEFAULT '1' COMMENT '是否有效',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=126 DEFAULT CHARSET=utf8;


